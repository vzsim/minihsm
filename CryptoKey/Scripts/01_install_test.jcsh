upload -c ${wrkDir}/../CryptoKey.cap
#upload -c ${wrkDir}/../../build/CryptoKey.cap

/echo ""
/echo "INSTALL IsoApplet"
install -i A00000000101 -q C9#() A000000001 A00000000101

/echo ""
/echo "PRINT THE LIST OF AVAILABLE PACKAGES AND APPLETS"
ls

